    "paths": {
      "@/*": ["src/*"],
      "@/components/*": ["src/components/*"],
      "@/layouts/*": ["src/layouts/*"],
      "@/pages/*": ["src/pages/*"],
      "@/icons/*": ["src/icons/*"],
      "@/lib/*": ["src/lib/*"]
